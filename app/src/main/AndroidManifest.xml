        <activity
            android:name=".KamKuSetActivity"
            android:label="@string/title_activity_kam_ku_set" />
        <activity
            android:name=".KamGroupSetActivity"
            android:label="@string/title_activity_kam_group_set" />
        <activity
            android:name=".SymbolSetActivity"
