        <activity
            android:name=".KamKuWordsActivity"
            android:label="@string/title_activity_kam_ku_words" />
        <activity
            android:name=".KamGroupWordsActivity"
            android:label="@string/title_activity_kam_group_words" />
        <activity
            android:name=".KamdiaoActivity"
            android:label="@string/title_activity_kamdiao" />
        <activity
            android:name=".KamkuActivity"
            android:label="@string/title_activity_kamku" />
        <activity
            android:name=".KamgroupActivity"
            android:label="@string/title_activity_kamgroup" />
        <activity
            android:name=".PopupTrue"
            android:label="@string/title_activity_popup_true" />
        <activity
            android:name=".PopupFalse"
            android:label="@string/title_activity_popup_false"></activity>
    </application>

</manifest>
